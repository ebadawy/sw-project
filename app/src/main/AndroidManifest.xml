<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.khaled.takequiz" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StudentHome"
            android:label="@string/title_activity_student_home"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.khaled.takequiz.MainActivity" />
        </activity>
        <activity
            android:name=".DoctorHome"
            android:label="@string/title_activity_doctor_home"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.khaled.takequiz.MainActivity" />
        </activity>
        <activity
            android:name=".QuizMaininfo"
            android:label="@string/title_activity_quiz_maininfo"
            android:parentActivityName=".DoctorHome" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.khaled.takequiz.DoctorHome" />
        </activity>
        <activity
            android:name=".QuizQuestionAnswerStudent"
            android:label="@string/title_activity_quiz_question_answer_student"
            android:parentActivityName=".QuizMaininfo" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.khaled.takequiz.QuizMaininfo" />
        </activity>
        <activity
            android:name=".QuizQuestionAnswerDoctor"
            android:label="@string/title_activity_quiz_question_answer_doctor"
            android:parentActivityName=".QuizMaininfo" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.khaled.takequiz.QuizMaininfo" />
        </activity>
    </application>

</manifest>
